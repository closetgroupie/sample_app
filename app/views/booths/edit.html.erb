<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for(@booth) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
 
	  <%= f.label :name, "change booth name" %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "save changes", class: "btn" %>
    <% end %>
  </div>
</div>
