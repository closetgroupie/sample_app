<% provide(:title, 'Search Results') %>

<h3 class ="text-center">Search results for <%= @q %>:</h3>

<div class="container">
  <div class="row">
    <% @items.each do |item| %>
    <h3>Items</h3>
    <%= render @items %>
    <% end %>
  </div>
</div>

<div class="container">
  <div class="row">
    <% @users.each do |user| %>
    <h3>Users</h3>
    <%= render @users %>
    <% end %>
  </div>
</div>

<div class="container">
  <div class="row">
    <% @booths.each do |booth| %>
    <h3>Booths</h3>
    <%= render @booths %>
    <% end %>
  </div>
</div>





